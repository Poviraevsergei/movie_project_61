<#macro userInfo user>
    <h1> Hello, this is your User!</h1>
    <h3>User id: ${user.id}</h3>
    <h3>User first name: ${user.firstName}></h3>
    <h3>User last name: ${user.lastName}</h3>
    <h3>User birthdate: ${user.birthdate}</h3>
    <h3>User login: ${user.login}</h3>
    <h3>User password: ${user.password}</h3>
    <h3>User email: ${user.email}</h3>
    <h3>User telephone: ${user.telephoneNumber}</h3>
</#macro>